{
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    },
    "@schematics/angular:application": {
      "strict": true
    }
  },
  "sourceRoot": "apps/patient-EHR/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "allowedCommonJsDependencies": [
          "crypto-js"
        ],
        "outputPath": "dist/apps/patient-EHR",
        "index": "apps/patient-EHR/src/index.html",
        "main": "apps/patient-EHR/src/main.ts",
        "polyfills": "apps/patient-EHR/src/polyfills.ts",
        "tsConfig": "apps/patient-EHR/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/patient-EHR/src/favicon.ico",
          "apps/patient-EHR/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/patient-EHR/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/patient-EHR/src/environments/environment.ts",
              "with": "apps/patient-EHR/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "patient-EHR:build:production"
        },
        "development": {
          "browserTarget": "patient-EHR:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "sslKey": "./ssl.key",
        "sslCert": "./ssl.cert"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "patient-EHR:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/patient-EHR/src/test.ts",
        "polyfills": "apps/patient-EHR/src/polyfills.ts",
        "tsConfig": "apps/patient-EHR/tsconfig.spec.json",
        "karmaConfig": "apps/patient-EHR/karma.conf.js",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/patient-EHR/src/favicon.ico",
          "apps/patient-EHR/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/patient-EHR/src/styles.scss"
        ],
        "scripts": []
      }
    }
  }
}
